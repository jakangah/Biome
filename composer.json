{
	"name": "mermetbt/biome",
	"description": "Biome Framework",
	"type": "library",
	"require": {
		"php": ">=5.5.0",
		"psr/log": "^1.0@dev",
		"koala-framework/composer-extra-assets": "^1.1@dev",
		"league/route": "^1.0@dev",
		"filp/whoops": "1.1.7",
		"sabre/xml": "1.2.0",
		"symfony/console": "2.7.6"
	},
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Timoth√© Mermet-Buffet",
			"email": "mermetbt@gmail.com"
		}
	],
	"minimum-stability": "dev",
	"autoload": {
		"psr-0": {
			"Biome": "src/"
		},
		"classmap": ["src/app/controllers/", "src/app/collections/", "src/app/models/", "src/app/commands/"]
	},
	"extra": {
		"require-bower": {
			"jquery": "*",
			"bootstrap": "*",
			"font-awesome": "*",
			"datatables": "*",
			"datatables-plugins": "*",
			"datatables-responsive-helper": "*",
			"select2-ng": "*"
		},
		"require-npm": {
			"gulp": "^3.9.0",
			"gulp-clean": "^0.3.1",
			"gulp-coffee": "^2.3.1",
			"gulp-concat": "^2.6.0",
			"gulp-concat-css": "^2.2.0",
			"gulp-cssmin": "^0.1.7",
			"gulp-rename": "^1.2.2",
			"gulp-uglify": "^1.4.2",
			"gulp-util": "^3.0.6"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"gulp build"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
