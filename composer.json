{
	"name": "mermetbt/biome",
	"description": "Biome Framework",
	"type": "library",
	"require": {
		"php": ">=5.5.0",
		"psr/log": "^1.0@dev",
		"koala-framework/composer-extra-assets": "^1.1@dev",
		"league/route": "^1.0@dev",
		"filp/whoops": "1.1.7",
		"sabre/xml": "1.2.0",
		"symfony/console": "2.7.6"
	},
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Timoth√© Mermet-Buffet",
			"email": "mermetbt@gmail.com"
		}
	],
	"minimum-stability": "dev",
	"autoload": {
		"psr-0": {
			"Biome": "src/"
		},
		"classmap": ["src/app/controllers/", "src/app/collections/", "src/app/models/", "src/app/commands/"]
	},
	"extra": {
		"require-bower": {
			"jquery": "*",
			"bootstrap": "*",
			"font-awesome": "*",
			"datatables": "*",
			"datatables-plugins": "*",
			"datatables-responsive-helper": "*",
			"select2-ng": "*",
			"autosize": "*"
		},
		"require-npm": {
			"gulp": "*",
			"gulp-rimraf": "*",
			"gulp-coffee": "*",
			"gulp-less": "*",
			"gulp-sass": "*",
			"gulp-concat": "*",
			"gulp-concat-css": "*",
			"gulp-cssmin": "*",
			"gulp-postcss": "*",
			"gulp-rucksack": "*",
			"gulp-rename": "*",
			"gulp-uglify": "*",
			"gulp-util": "*",
			"gulp-chug": "*",
			"precss": "*",
			"rucksack-css": "*"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"gulp build"
		],
		"post-update-cmd": [
			"gulp build"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "stable"
}
